{
  "jobEmailSources": [
    {
      "emailSender": "info@jobs.totaljobsmail.com",
      "jobLinkPatterns": ["https://click.totaljobsmail.com/*"],
      "followJobLink": true,
      "linkSelector": "//a",
      "linkTextExclusions": ["unsubscribe", "view in browser", "privacy policy", "manage preferences"]
    },
    {
      "emailSender": "jobs-listings@linkedin.com",
      "jobLinkPatterns": ["https://www.linkedin.com/comm/*", "https://linkedin.com/jobs/*"],
      "followJobLink": true,
      "linkSelector": "//a",
      "linkTextExclusions": ["unsubscribe", "settings", "help", "privacy"]
    }
  ],
  "delayBetweenRequests": 100
}
